nextflow_pipeline {

    name "Test Workflow main.nf"
    script "main.nf"

    test("Should run without failures") {

        when {
        params {
            load("$baseDir/tests/testdata/test_params.json")
        }
        }

        then {
            assert workflow.success
            assert snapshot(workflow.out.cohort_maf).match()
            assert snapshot(workflow.out.summary_files).match()

        }

    }

}
